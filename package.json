{
  "name": "ng-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_docs": "ng build --output-path=docs",
    "build_dist": "ng build --output-path=dist --base-href='https://liuyongjin.gitee.io/ng-blog/'",
    "watch": "ng build --watch --configuration development",
