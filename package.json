    "dev:ssr": "ng run ng-blog:serve-ssr",
    "serve:ssr": "node dist/ng-blog/server/main.js",
    "build:ssr": "ng build && ng run ng-blog:server",
    "prerender": "ng run ng-blog:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
